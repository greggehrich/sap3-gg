<%= form_tag( "url_show" ) do %>
    <div>
      <br><br><br> <%= label_tag 'Enter Source URL -->' %>
      <%= text_field_tag(:source_url, @source_url, size:50) %><br>
      <%= text_field_tag(:domain, @domain, size:50) %>
      <%= text_field_tag(:full_web_url, @full_web_url, size:50) %><br>
      <%= text_field_tag(:meta_desc_scrape, @meta_desc_scrape, size:100) %><br>
      <%= text_field_tag(:meta_keyword_scrape, @meta_keyword_scrape, size:100) %>
      <br>
    </div>
    <div>
      <%= button_to "Process URL", { :class => 'btn btn-primary btn-sm' } %>
      <br>
    </div>
    <div>
      source url is: <%= @source_url %><br>
      domain is: <%= @domain %><br>
      full web url is: <%= @full_web_url %><br>
      meta desc is: <%= @meta_desc_scrape %><br>
      meta keyword is: <%= @meta_keyword_scrape %><br>
      timeout is: <%= @timeout_flg %><br>
      <br>
    </div>
    <iframe src="<%= @full_web_url %>" frameborder="1" width="100%" height="400px">error!</iframe>
<% end %>
